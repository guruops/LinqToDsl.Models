<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <Authors>Invictus Gurus, LLC</Authors>
    <Product>GuruOps.ES.LinqToDsl.Models</Product>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>0.1.2</Version>
    <FileVersion>0.1.2.0</FileVersion>
    <AssemblyVersion>0.1.2.0</AssemblyVersion>
    <Company>Invictus Gurus, LLC</Company>
    <LangVersion>9</LangVersion>
    <Copyright>Invictus Gurus, LLC</Copyright>
    <PackageProjectUrl>https://github.com/guruops/LinqToDsl.Models</PackageProjectUrl>
    <Description>GuruOps.ES.LinqToDsl.Models - common models</Description>
  </PropertyGroup>

  <PropertyGroup>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageReleaseNotes>0.1.2 - Initial release + models update</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/guruops/LinqToDsl.Models</RepositoryUrl>
    <PackageTags>InvictusGurus GuruOps LinqToDsl</PackageTags>
    <RepositoryType>git</RepositoryType>
    <Title>GuruOps.ES.LinqToDsl.Models</Title>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
    <PackageReference Include="System.Text.Json" Version="5.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
